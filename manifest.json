{
    "name": "OAuth2 Attachment to Drive",
    "version": "0.1",
    "description": "OAuth connect to Google People API, Gmail and google drive API. attach to drive",
    "manifest_version": 3,
    "key":"",
    "oauth2":{
        "client_id":"433235437826-c6nbbqv49uanuav5rtfpof6av6grovo4.apps.googleusercontent.com",
        "scopes":[
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/contacts.readonly",
            
            "https://mail.google.com/",
            "https://www.googleapis.com/auth/gmail.modify"
            // "https://www.googleapis.com/auth/gmail.compose",

            // "https://www.googleapis.com/auth/gmail.labels",

            // "https://www.googleapis.com/auth/drive",
            // "https://www.googleapis.com/auth/drive.appdata",
            // "https://www.googleapis.com/auth/drive.apps.readonly",
            // "https://www.googleapis.com/auth/drive.file",
            // "https://www.googleapis.com/auth/drive.metadata",
            // "https://www.googleapis.com/auth/drive.photos.readonly",
            // "https://www.googleapis.com/auth/drive.appfolder",
            // "https://www.googleapis.com/auth/drive.metadata.readonly",
            // "https://www.googleapis.com/auth/drive.install",
            // "https://www.googleapis.com/auth/drive.photos.readonly",
            // "https://www.googleapis.com/auth/drive.readonly"
        ]
    },
    "permissions": [
        "identity",
        "activeTab",
        "tabs", 
        "scripting"
    ],
    "action": {
        "default_title": "FriendBlock 3.0, friends face's in a block."
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
    {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
    }
    ],
    "icons":{
        "16": "images/Upload.png"
    }
}